<h1>New User</h1>

<form action="<%= users_url %>" method="POST">
    
    <%auth_token%>
    <label for="new_user_name">Name</label>
    <input id="new_user_name" type="text" name="user[name]">
    <label for="new_user_password">Password</label>
    <input id="new_user_password" type="password" name="user[password]">

    <input type="submit" value="Create User">
</form> 